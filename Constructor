package midtermtp01;

/**
 *
 * @author user
 */
public class MidtermTP01 {
    public class Student{
        
    private String name;
    private String section;
    private int grade;
        
        public class Student(String name){
            this.name=name;
        }
        public class Student(String name,String section){
            this.name=name;
            this.section=section;
        }
        public class Student(String name,String section,int grade){
            this.name=name;
            this.section=section;
            this.grade=grade;
        }
        }
        
    public static void main(String[] args) {
       Student Mt=new Student("Luffy");
       Student Mt=new Student("Zoro","BSIT");
       Student Mt=new Student("Sanji","BSCS",98);
        
        
    
    }
}
