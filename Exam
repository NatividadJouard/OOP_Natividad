package school;

/**
 *
 * @author eduna
 */
class Exam {
    String message;
    double price;
    boolean status;

    Exam() {
        message = "Good luck";
    }

    Exam(String period, String level) {
    }

    double getPrice() {
        return price;
    }

    boolean isFinished() {
        return status;
    }
}

class Midterm extends Exam {
    Midterm() {
        super();
        System.out.println("Exam has started.");
    }
}

class Essay extends Midterm {
}
